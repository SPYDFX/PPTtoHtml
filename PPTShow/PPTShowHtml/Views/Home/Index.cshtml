@{
    ViewBag.Title = "Home Page";
}
@section Scripts {
    @Scripts.Render("~/bundles/FileUpload")
}
@section Css{
    @Styles.Render("~/Content/FileUpload")
}
<div class="jumbotron">
    <h1>上传图片</h1>
    <div class="form-group clearfix">
        <div class="col-lg-10 col-md-10 col-sm-10 clearfix">
            <!-- 图片展示 -->
            <form enctype="multipart/form-data" id="form1">

                <div class="atlas-container pull-left clearfix">
                  
                </div>
            </form>
            <!-- 图片上传 -->
            <div class="upload-container pull-left">
                <form id="postForm">
                    <input id="fileUpload" type="file" name="upload_img" multiple/>
                    <div class="fileUpload"></div>
                </form>
            </div>
        </div>
    </div>
    <div class="col-lg-10 col-md-10 col-sm-10">
        <p><a href="javascript:void(0)" class="btn btn-primary btn-lg" id="btnUploadImg">上传图片</a></p>
    </div>
   
</div>
